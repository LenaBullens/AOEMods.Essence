<Application x:Class="AOEMods.Essence.Editor.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:AOEMods.Essence.Editor"
             Startup="OnStartup">
    <Application.Resources>
        <local:BooleanToVisibilityConverter x:Key="BoolToVis" TrueValue="Visible" FalseValue="Collapsed" />
        <local:BooleanToVisibilityConverter x:Key="NotBoolToVis" TrueValue="Collapsed" FalseValue="Visible" />
    </Application.Resources>
</Application>
